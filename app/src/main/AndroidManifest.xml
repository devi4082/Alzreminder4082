<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reyna.alzreminder">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".progressActivity" />
        <activity android:name=".rememberActivity" />
        <activity android:name=".nextTaskActivity" />
        <activity android:name=".addTaskActivity" />
        <activity android:name=".viewTaskActivity" />
        <activity android:name=".questionActivity" />
        <activity android:name=".myMomentActivity" />
        <activity android:name=".addMomentActivity" />
        <activity android:name=".viewMomentActivity"></activity>
    </application>

</manifest>